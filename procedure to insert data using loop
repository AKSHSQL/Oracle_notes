create or replace procedure add_data
is
begin
 for i in 1..50 loop
  insert into asdf values (i);
 end loop;
 dbms_output.put_line('ok');
end add_data;

exec add_data;



create or replace procedure rep_str (str1 in varchar2)
is
str varchar2(1000);
len number;
begin
   len := length(str1);
   for i in 1..len loop
    for j in 1..i loop
    str:=substr(str1,1,i);
    end loop;
    dbms_output.put_line(str);
    end loop;
    str:=null;
       len := length(str1);
   
   for i in reverse 1..len loop
    for j in 1..i loop
    str:=substr(str1,1,i);
    end loop;
    dbms_output.put_line(str);
    end loop;
    str:=null;
end rep_str;
